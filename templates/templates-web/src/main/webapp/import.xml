<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
         xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:nt="http://www.jcp.org/jcr/nt/1.0">


    <templatesSet>
        <templates-web jcr:primaryType="jnt:templatesSetFolder">
            <defaultSite jcr:primaryType="jnt:virtualsite" j:installedModules="templates-web">
                <contents jcr:primaryType="jnt:folder">
                    <site_components jcr:primaryType="jnt:folder">
                    </site_components>
                </contents>
                <files jcr:primaryType="jnt:folder"/>
                <mashups jcr:primaryType="jnt:folder"/>
                <themes jcr:primaryType="jnt:folder"/>
                <home j:template="default" jcr:primaryType="jnt:page">
                    <j:translation jcr:primaryType="jnt:translation" jcr:title_en="Home" jcr:language="en"/>
                    <pagecontent jcr:primaryType="jnt:contentList" jcr:mixinTypes="jmix:templateInformation"  j:templateLocked="true">
                        <row1 jcr:primaryType="jnt:row" column="2col106" jcr:mixinTypes="jmix:templateInformation"  j:templateLocked="true">
                            <col1 jcr:primaryType="jnt:contentList"/>
                            <col2 jcr:primaryType="jnt:contentList">

                            </col2>
                        </row1>
                    </pagecontent>
                </home>
                <templates jcr:primaryType="jnt:templatesFolder">
                    <simple j:template="default" jcr:primaryType="jnt:page">
                        <j:translation jcr:primaryType="jnt:translation" jcr:title_en="Simple" jcr:language="en"/>
                        <pagecontent jcr:primaryType="jnt:contentList" jcr:mixinTypes="jmix:templateInformation" j:templateLocked="true">
                            <row1 jcr:primaryType="jnt:row" column="2col124" jcr:mixinTypes="jmix:templateInformation" j:templateLocked="true">
                                <col1 jcr:primaryType="jnt:contentList"/>
                                <col2 jcr:primaryType="jnt:contentList"/>                                
                            </row1>
                        </pagecontent>
                    </simple>
                </templates>
            </defaultSite>
        </templates-web>
    </templatesSet>
</content>
