<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0">
  <templatesSet>
    <templates-web xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" jcr:mixinTypes="jmix:templateInformation jmix:sitemap jmix:accessControlled jmix:wrapper" jcr:primaryType="jnt:virtualsite" changefreq="monthly" j:installedModules="templates-web" priority="0.5">
      <contents jcr:mixinTypes="jmix:templateInformation jmix:wrapper" jcr:primaryType="jnt:contentList">
        <site_components jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
          <simple-search result="/templatesSet/templates-web/search-results" jcr:mixinTypes="jmix:templateInformation jmix:shareable" jcr:primaryType="jnt:simpleSearchForm" j:templateLocked="true">
            <j:translation_en jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:translation" jcr:title_en="Simple search" jcr:language="en" />
          </simple-search>
          <side-menu j:menuType="primary" jcr:mixinTypes="jmix:templateInformation jmix:shareable" j:maxDepth="3" jcr:primaryType="jnt:navMenu" j:baselineNode="/templatesSet/templates-web/home" j:startLevel="1" j:styleName="navigationN2" />
        </site_components>
      </contents>
      <files jcr:mixinTypes="jmix:templateInformation jmix:wrapper" jcr:primaryType="jnt:folder" />
      <mashups jcr:mixinTypes="jmix:templateInformation jmix:wrapper" jcr:primaryType="jnt:portletList" />
      <themes jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:folder" />
      <home jcr:mixinTypes="jmix:templateInformation jmix:sitemap" j:template="default" jcr:primaryType="jnt:page" j:displayInMenu="primary" changefreq="monthly" priority="0.5">
        <j:translation_en jcr:language="en" jcr:title_en="Home" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:translation" />
        <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" j:templateLocked="true">
          <row1 column="2col115" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row" j:templateLocked="true">
            <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" />
            <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
              <simple-search j:share="/templatesSet/templates-web/contents/site_components/simple-search" />
            </col2>
          </row1>
        </pagecontent>
      </home>
      <templates jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:templatesFolder">
        <simple jcr:mixinTypes="jmix:templateInformation jmix:sitemap" j:template="default" jcr:primaryType="jnt:template" j:displayInMenu="primary" changefreq="monthly" priority="0.5">
          <j:translation_en jcr:language="en" jcr:title_en="Simple" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:translation" />
          <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" j:templateLocked="true">
            <row1 column="2col115" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row" j:templateLocked="true">
              <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" />
              <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                <simple-search j:share="/templatesSet/templates-web/contents/site_components/simple-search" />
                <side-menu j:share="/templatesSet/templates-web/contents/site_components/side-menu" />
              </col2>
            </row1>
          </pagecontent>
        </simple>
        <my-portal jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:template" j:displayInMenu="primary" changefreq="monthly" priority="0.5">
          <j:translation_en jcr:title_en="My Portal" jcr:language="en" jcr:primaryType="jnt:translation" />
          <pagecontent jcr:primaryType="jnt:contentList">
            <my-portal jcr:primaryType="jnt:contentList">
              <j:translation_en jcr:title_en="My Portal" jcr:language="en" jcr:primaryType="jnt:translation" />
              <currentUserDisplay jcr:primaryType="jnt:currentUserDisplay" j:referenceTemplate="portal" />
            </my-portal>
          </pagecontent>
        </my-portal>
        <events jcr:mixinTypes="jmix:templateInformation jmix:sitemap" j:template="default" j:displayInMenu="primary" jcr:primaryType="jnt:template" changefreq="monthly" priority="0.5">
          <j:translation_en j:template="default" jcr:primaryType="jnt:translation" j:displayInMenu="primary" changefreq="monthly" jcr:language="en" jcr:title_en="Events" priority="0.5" />
          <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" j:templateLocked="true">
            <row1 column="2col115" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row" j:templateLocked="true">
              <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                <events jcr:mixinTypes="jmix:listRestrictions" jcr:primaryType="jnt:contentList" j:allowedTypes="jnt:event">
                  <j:translation_en jcr:primaryType="jnt:translation" jcr:language="en" j:allowedTypes="jnt:event" jcr:title_en="Events" />
                </events>
              </col1>
              <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                <simple-search j:share="/templatesSet/templates-web/contents/site_components/simple-search" />
                <side-menu j:share="/templatesSet/templates-web/contents/site_components/side-menu" />
                <calendar startDateProperty="startDate" endDateProperty="endDate" jcr:primaryType="jnt:calendar" j:bindedComponent="/templatesSet/templates-web/templates/events/pagecontent/row1/col1/events" />
                <facets jcr:primaryType="jnt:facets" j:bindedComponent="/templatesSet/templates-web/templates/events/pagecontent/row1/col1/events">
                  <fieldFacet mincount="1" field="jnt:event;eventsType" jcr:primaryType="jnt:fieldFacet" missing="false" facet="type" offset="0" limit="100">
                    <j:translation_en mincount="1" field="jnt:event;eventsType" jcr:primaryType="jnt:translation" missing="false" jcr:language="en" facet="type" label_en="Type" offset="0" limit="100" />
                  </fieldFacet>
                </facets>
              </col2>
            </row1>
          </pagecontent>
        </events>
      </templates>
      <search-results jcr:mixinTypes="jmix:templateInformation jmix:sitemap" j:template="default" jcr:primaryType="jnt:page" j:displayInMenu="primary" changefreq="monthly" j:sourceTemplate="/templatesSet/templates-web/templates/simple" priority="0.5">
        <j:translation_en j:template="default" jcr:primaryType="jnt:translation" changefreq="monthly" jcr:language="en" jcr:title_en="Search results" priority="0.5" />
        <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" j:templateLocked="true">
          <row1 column="1col16" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row" j:templateLocked="true">
            <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
              <simple-search j:share="/templatesSet/templates-web/contents/site_components/simple-search" />
              <results autoSuggest="true" pageSize="10" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:searchResults" autoSuggestHitCount="2" autoSuggestMinimumHitCount="2">
                <j:translation_en autoSuggest="true" pageSize="10" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:translation" jcr:title_en="Results" autoSuggestHitCount="2" jcr:language="en" autoSuggestMinimumHitCount="2" />
              </results>
            </col1>
            <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList" />
          </row1>
        </pagecontent>
      </search-results>
      <sitewrapper j:template="web.bodywrapper" jcr:primaryType="jnt:wrapper" j:key="bodywrapper">
        <header jcr:primaryType="jnt:contentList">
          <shortcuts j:templateShared="false" jcr:mixinTypes="jmix:renderable jmix:templateInformation jmix:shareable" j:template="inline" jcr:primaryType="jnt:shortcuts" j:templateLocked="true" />
        </header>
        <logo jcr:primaryType="jnt:contentList" />
        <topMenu jcr:primaryType="jnt:contentList">
          <top-menu j:menuType="primary" jcr:mixinTypes="jmix:templateInformation jmix:shareable" j:maxDepth="1" jcr:primaryType="jnt:navMenu" j:baselineNode="/templatesSet/templates-web/home" j:styleName="navigationN1" />
        </topMenu>
        <logoFooter jcr:primaryType="jnt:contentList" />
        <footer jcr:primaryType="jnt:contentList" />
        <wrappercontent jcr:primaryType="jnt:contentList">
          <mainResourceDisplay jcr:primaryType="jnt:mainResourceDisplay" />
        </wrappercontent>
      </sitewrapper>
      <j:acl jcr:primaryType="jnt:acl">
        <GRANT_r_editor j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false" j:principal="r:editor" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_editor j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false" j:principal="r:editor" j:aceType="DENY" jcr:primaryType="jnt:ace" />
        <GRANT_r_editor-in-chief j:privileges="jcr:read jcr:write_live jcr:modifyAccessControl jcr:read_live jcr:write" j:protected="false" j:principal="r:editor-in-chief" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_editor-in-chief j:privileges="" j:protected="false" j:principal="r:editor-in-chief" j:aceType="DENY" jcr:primaryType="jnt:ace" />
        <GRANT_r_publisher j:privileges="jcr:read jcr:write_live jcr:read_live jcr:write" j:protected="false" j:principal="r:publisher" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_publisher j:privileges="jcr:modifyAccessControl" j:protected="false" j:principal="r:publisher" j:aceType="DENY" jcr:primaryType="jnt:ace" />
        <GRANT_r_seo-manager j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false" j:principal="r:seo-manager" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_seo-manager j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false" j:principal="r:seo-manager" j:aceType="DENY" jcr:primaryType="jnt:ace" />
        <GRANT_r_site-administrator j:privileges="jcr:read jcr:write_live jcr:modifyAccessControl jcr:read_live jcr:write" j:protected="false" j:principal="r:site-administrator" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_site-administrator j:privileges="" j:protected="false" j:principal="r:site-administrator" j:aceType="DENY" jcr:primaryType="jnt:ace" />
        <GRANT_r_translator j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false" j:principal="r:translator" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_translator j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false" j:principal="r:translator" j:aceType="DENY" jcr:primaryType="jnt:ace" />
        <GRANT_r_web-designer j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false" j:principal="r:web-designer" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        <DENY_r_web-designer j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false" j:principal="r:web-designer" j:aceType="DENY" jcr:primaryType="jnt:ace" />
      </j:acl>
      <workflowrules jcr:primaryType="jnt:workflowRules">
        <nt_base_jBPM_1-step-publication j:nodeType="nt:base" j:workflow="jBPM:1-step-publication" jcr:primaryType="jnt:workflowRule">
          <GRANT_r_editor-in-chief j:privileges="review start" j:protected="false" j:principal="r:editor-in-chief" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
          <GRANT_r_editor j:privileges="start" j:protected="false" j:principal="r:editor" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
          <GRANT_r_publisher j:privileges="review" j:protected="false" j:principal="r:publisher" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
        </nt_base_jBPM_1-step-publication>
      </workflowrules>
    </templates-web>
  </templatesSet>
</content>
