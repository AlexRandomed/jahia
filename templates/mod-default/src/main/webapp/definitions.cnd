[jmix:sendAsNewsletter] mixin

[jmix:filteredList] > jmix:contentListMixin mixin
 sharedSmallText filter (choicelist[resourceBundle]) indexed=no < 'jcr:category','jcr:author'

[jnt:fileList] > jnt:container, jmix:contentListMixin mixin
 file folder (file[folder])

[jnt:navLink] > jnt:container
 page navLink primary
 smallText separator

[jnt:image] > jnt:container    jmix:structuredContent
file image (file[mime='image/*'])

[jnt:frame] > jnt:container     jmix:structuredContent
 sharedSmallText source indexed=no
 sharedSmallText name indexed=no
 integer width indexed=no
 integer height indexed=no
 integer frameborder = '0' indexed=no
 integer marginwidth = '0' indexed=no
 integer marginheight = '0' indexed=no
 sharedSmallText scrolling (choicelist) indexed=no < 'yes','no','auto'
 smallText alt indexed=no

[jnt:video] > jnt:container, jmix:interactiveContent
 sharedSmallText name
 file source
 integer width = '0'
 integer height = '0'
 integer hspace = '0'
 integer vspace = '0'
 sharedSmallText autostart (choicelist) analyzer='keyword' nofulltext < 'true','false'
 sharedSmallText invokeURLs (choicelist) analyzer='keyword' nofulltext < 'true','false'
 sharedSmallText enablecontextmenu (choicelist) analyzer='keyword' nofulltext < '0','1'
 sharedSmallText showstatusbar (choicelist) analyzer='keyword' nofulltext < '0','1'
 sharedSmallText showcontrols (choicelist) analyzer='keyword' nofulltext < '0','1'
 sharedSmallText autosize (choicelist) analyzer='keyword' nofulltext < 'true','false'
 integer displaysize = '0' analyzer='keyword' nofulltext
 sharedSmallText loop (choicelist) analyzer='keyword' nofulltext < 'true','false'

[jnt:flash] > jnt:container, jmix:interactiveContent
 file flashSource
 sharedSmallText width analyzer='keyword' nofulltext
 sharedSmallText height analyzer='keyword' nofulltext
