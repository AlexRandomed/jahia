<?xml version="1.0" encoding="UTF-8"?>

<template-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.jahia.org/jahia/templates" xsi:schemaLocation="http://www.jahia.org/jahia/templates http://www.jahia.org/shemas/templates_2_0.xsd">
    <package-name>Social templates</package-name>
    <root-folder>social-templates</root-folder>
    <provider>http://www.jahia.org</provider>
    <thumbnail>images/preview.png</thumbnail>
    <resource-bundle>resources.SocialTemplates</resource-bundle>
    <definitions-file>definitions.cnd</definitions-file>
    <common-pages>
        <search-results>searchresult.jsp</search-results>
    </common-pages>
    <templates default="Blog" homepage="blog">
        <template name="Blog" display-name="template.name.blog" filename="tpl.blog.jsp" page-type="social_templates:blog" description="template.description.blog"/>
    </templates>
</template-set>

