<jnt = 'http://www.jahia.org/jahia/nt/1.0'>

[jnt:feed] > jnt:content, mix:title, jmix:editorialContent, jmix:list
 - url (string)
 - user (string)
 - password (string)
 - type (string) < newsml
 - syncro (string)
 + medias (jnt:folder)
 + * (jnt:newsMLItem)

[jnt:newsMLItem] > jnt:news
 - itemID (string)
 - status (string)
 - publicIdentifier (string)
 - subjectCode (string)
 - urgency (long)
 - contentUpdated (date) hidden
 + * (jnt:newsMLComponent)

[jnt:newsMLComponentOrContentItem] > jnt:content, mix:title

[jnt:newsMLComponent] > jnt:newsMLComponentOrContentItem
 - Duid (string)
 - language (string)
 - subjectCode (string)
 - headline (string)
 - role (string)
 + * (jnt:newsMLComponentOrContentItem)

[jnt:newsMLContentItem] > jnt:newsMLComponentOrContentItem, jmix:editorialContent
 - comment (string)
 - format (string)
 - href (string)
 - mediatype (string)
 - datacontent (string)
 - role (string)
 - image (weakreference, file[mime='image/*'])
