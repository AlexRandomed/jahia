<jnt = 'http://www.jahia.org/jahia/nt/1.0'>

[jnt:feed] > jnt:content, mix:title, jmix:editorialContent
 - url (string)
 - user (string)
 - password (string)
 - type (string) < newsml
 - syncro (string)
 + * (jnt:newsMLItem)

[jnt:newsMLItem] > jnt:news
 - itemID (string)
 - status (string)
 - publicIdentifier (string)
 - subjectCode (string)
 - urgency (long)
 - contentUpdated (date) hidden
 + * (jnt:feedContent)

[jnt:feedContent] > nt:unstructured
