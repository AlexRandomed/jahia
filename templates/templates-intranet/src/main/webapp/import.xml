<content xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <templatesSet>
        <templates-intranet xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
                            xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
                            xmlns:mix="http://www.jcp.org/jcr/mix/1.0" jcr:mixinTypes="jmix:sitemap jmix:wrapper"
                            jcr:primaryType="jnt:virtualsite" changefreq="monthly"
                            j:installedModules="templates-intranet" priority="0.5">
            <contents jcr:mixinTypes="jmix:wrapper" jcr:primaryType="jnt:contentList">
                <site_components jcr:primaryType="jnt:contentList">
                    <login-to-jahia jcr:mixinTypes="jmix:templateInformation jmix:shareable"
                                    jcr:primaryType="jnt:loginForm" j:templateLocked="true">
                        <j:translation_en jcr:primaryType="jnt:translation" jcr:language="en"
                                          jcr:title_en="Login to Jahia"/>
                    </login-to-jahia>
                </site_components>
            </contents>
            <files jcr:mixinTypes="jmix:wrapper" jcr:primaryType="jnt:folder"/>
            <mashups jcr:mixinTypes="jmix:wrapper" jcr:primaryType="jnt:portletList"/>
            <themes jcr:primaryType="jnt:folder"/>
            <home jcr:mixinTypes="jmix:sitemap" j:template="default" jcr:primaryType="jnt:page" changefreq="monthly"
                  priority="0.5">
                <j:translation_en jcr:language="en" jcr:title_en="Home" jcr:primaryType="jnt:translation"/>
                <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList"
                             j:templateLocked="true">
                    <row1 column="2col106" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row"
                          j:templateLocked="true">
                        <col1 jcr:primaryType="jnt:contentList"/>
                        <col2 jcr:primaryType="jnt:contentList">
                            <login-to-jahia
                                    j:share="/templatesSet/templates-intranet/contents/site_components/login-to-jahia"/>
                        </col2>
                    </row1>
                </pagecontent>
            </home>
            <templates jcr:primaryType="jnt:templatesFolder">
                <simple jcr:mixinTypes="jmix:sitemap" j:template="default" jcr:primaryType="jnt:template"
                        changefreq="monthly" priority="0.5">
                    <j:translation_en jcr:language="en" jcr:title_en="Simple" jcr:primaryType="jnt:translation"/>
                    <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList"
                                 j:templateLocked="true">
                        <row1 column="2col124" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row"
                              j:templateLocked="true">
                            <col1 jcr:primaryType="jnt:contentList"/>
                            <col2 jcr:primaryType="jnt:contentList"/>
                        </row1>
                    </pagecontent>
                </simple>
                <double jcr:mixinTypes="jmix:sitemap" j:template="default" jcr:primaryType="jnt:template"
                        changefreq="monthly" priority="0.5">
                    <j:translation_en jcr:title_en="Double" jcr:language="en" jcr:primaryType="jnt:translation"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <row1 column="3col664" jcr:primaryType="jnt:row">
                            <col1 jcr:primaryType="jnt:contentList"/>
                            <col2 jcr:primaryType="jnt:contentList"/>
                            <col3 jcr:primaryType="jnt:contentList"/>
                        </row1>
                    </pagecontent>
                </double>
                <docspace jcr:mixinTypes="jmix:templateInformation jmix:sitemap" jcr:primaryType="jnt:template"
                          changefreq="monthly" priority="0.5">
                    <j:translation_en jcr:title_en="Docspace" jcr:language="en"
                                      jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:translation"/>
                    <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                        <row column="2col88" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row">
                            <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                <docspaceTree jcr:mixinTypes="jmix:templateInformation"
                                              jcr:primaryType="jnt:docspaceTree"/>
                            </col1>
                            <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                <docspaceFormCreation jcr:mixinTypes="jmix:templateInformation"
                                                      jcr:primaryType="jnt:docspaceFormCreation"/>
                                <contentList jcr:mixinTypes="jmix:renderable jmix:templateInformation"
                                             j:template="twoColumns" jcr:primaryType="jnt:contentList">
                                    <lastDocspace jcr:mixinTypes="jmix:templateInformation"
                                                  startNode="/templatesSet/templates-intranet/templates/docspace"
                                                  jcr:primaryType="jnt:lastDocspace"/>
                                    <lastDocspaceDoc jcr:mixinTypes="jmix:templateInformation"
                                                     startNode="/templatesSet/templates-intranet/templates/docspace"
                                                     jcr:primaryType="jnt:lastDocspaceDoc"/>
                                </contentList>
                            </col2>
                        </row>
                    </pagecontent>
                    <wrapper jcr:mixinTypes="jmix:templateInformation" j:template="default"
                             jcr:primaryType="jnt:wrapper" j:key="folderWrapper" j:applyOn="jnt:folder">
                        <wrappercontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                            <row column="2col124" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row">
                                <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <mainResourceDisplay jcr:mixinTypes="jmix:templateInformation"
                                                         jcr:primaryType="jnt:mainResourceDisplay"
                                                         j:referenceTemplate="docspace.detail"/>
                                </col1>
                                <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <aclUsers jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:aclUsers"
                                              j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper/wrappercontent/row/col1/mainResourceDisplay"/>
                                    <aclUsers-1 jcr:mixinTypes="jmix:renderable jmix:templateInformation"
                                                j:template="add" jcr:primaryType="jnt:aclUsers"
                                                j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper/wrappercontent/row/col1/mainResourceDisplay"/>
                                    <lastDocspace jcr:mixinTypes="jmix:templateInformation"
                                                  startNode="/templatesSet/templates-intranet/templates/docspace"
                                                  jcr:primaryType="jnt:lastDocspace"/>
                                </col2>
                            </row>
                            <row-1 column="1col16" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row">
                                <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <mainResourceDisplay jcr:mixinTypes="jmix:templateInformation"
                                                         jcr:primaryType="jnt:mainResourceDisplay"
                                                         j:referenceTemplate="docspace"/>
                                </col1>
                            </row-1>
                        </wrappercontent>
                    </wrapper>
                    <wrapper-1 jcr:mixinTypes="jmix:templateInformation" j:template="default"
                               jcr:primaryType="jnt:wrapper" j:key="fileWrapper" j:applyOn="jnt:file">
                        <wrappercontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                            <row column="2col124" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row">
                                <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <mainResourceDisplay jcr:mixinTypes="jmix:templateInformation"
                                                         jcr:primaryType="jnt:mainResourceDisplay"
                                                         j:referenceTemplate="docspace"/>
                                    <mainResourceDisplay-1 jcr:mixinTypes="jmix:templateInformation"
                                                           jcr:primaryType="jnt:mainResourceDisplay"
                                                           j:referenceTemplate="updateFile"/>
                                    <document-history j:skin="skins.box2"
                                                      jcr:mixinTypes="jmix:templateInformation jmix:skinnable"
                                                      jcr:primaryType="jnt:contentList">
                                        <j:translation_en j:skin="skins.box2" jcr:language="en"
                                                          jcr:title_en="Document History"
                                                          jcr:mixinTypes="jmix:templateInformation"
                                                          jcr:primaryType="jnt:translation"/>
                                        <commentable jcr:mixinTypes="jmix:templateInformation"
                                                     jcr:primaryType="jnt:commentable"
                                                     j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper-1/wrappercontent/row/col1/mainResourceDisplay-1"/>
                                        <commentable-1 jcr:mixinTypes="jmix:renderable jmix:templateInformation"
                                                       j:template="addcomment" jcr:primaryType="jnt:commentable"
                                                       j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper-1/wrappercontent/row/col1/mainResourceDisplay-1"/>
                                    </document-history>
                                </col1>
                                <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <displayVersion jcr:mixinTypes="jmix:templateInformation"
                                                    jcr:primaryType="jnt:displayVersion"
                                                    j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper-1/wrappercontent/row/col1/mainResourceDisplay"/>
                                    <extras j:skin="skins.box5style2" jcr:mixinTypes="jmix:skinnable"
                                            jcr:primaryType="jnt:contentList">
                                        <j:translation_en j:skin="skins.box5style2" jcr:language="en"
                                                          jcr:title_en="Extras" jcr:primaryType="jnt:translation"/>
                                        <rateable jcr:mixinTypes="jmix:templateInformation"
                                                  jcr:primaryType="jnt:rateable"
                                                  j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper-1/wrappercontent/row/col1/mainResourceDisplay-1"/>
                                        <tag-this-document jcr:mixinTypes="jmix:templateInformation"
                                                           jcr:primaryType="jnt:tagging"
                                                           j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper-1/wrappercontent/row/col1/mainResourceDisplay-1">
                                            <j:translation_en jcr:title_en="Tag This Document" jcr:language="en"
                                                              jcr:mixinTypes="jmix:templateInformation"
                                                              jcr:primaryType="jnt:translation"/>
                                        </tag-this-document>
                                        <tag-this-document-1 jcr:mixinTypes="jmix:renderable jmix:templateInformation"
                                                             j:template="addtag" jcr:primaryType="jnt:tagging"
                                                             j:bindedComponent="/templatesSet/templates-intranet/templates/docspace/wrapper-1/wrappercontent/row/col1/mainResourceDisplay-1">
                                            <j:translation_en jcr:title_en="Tag this document" jcr:language="en"
                                                              j:template="addtag" jcr:primaryType="jnt:translation"/>
                                        </tag-this-document-1>
                                    </extras>
                                </col2>
                            </row>
                        </wrappercontent>
                    </wrapper-1>
                </docspace>
                <wiki jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:template" changefreq="monthly" priority="0.5">
                    <j:translation_en jcr:title_en="Wiki" jcr:language="en" jcr:primaryType="jnt:translation"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <row column="2col412" jcr:primaryType="jnt:row">
                            <col2 j:subNodesTemplate="link" j:skin="skins.box2style1"
                                  jcr:mixinTypes="jmix:renderableList jmix:skinnable" jcr:primaryType="jnt:contentList">
                                <j:translation_en j:subNodesTemplate="link" j:skin="skins.box2style1"
                                                  jcr:title_en="Create Wiki Pages" jcr:language="en"
                                                  jcr:primaryType="jnt:translation"/>
                                <create-a-wiki-page jcr:primaryType="jnt:wikiPageFormCreation">
                                    <j:translation_en jcr:language="en" jcr:title_en="Create a wiki page"
                                                      jcr:primaryType="jnt:translation"/>
                                </create-a-wiki-page>
                            </col2>
                            <col1 j:skin="skins.box5" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                                <j:translation_en j:skin="skins.box5" jcr:language="en" jcr:title_en="Latest Wiki Pages"
                                                  jcr:primaryType="jnt:translation"/>
                                <latestContent j:criteria="lastModified" jcr:mixinTypes="jmix:renderable"
                                               j:template="linkList" j:type="jnt:wikiPage" j:nbOfResult="20"
                                               jcr:primaryType="jnt:latestContent"
                                               startNode="/templatesSet/templates-intranet/templates/wiki"/>
                            </col1>
                        </row>
                    </pagecontent>
                    <wrapper j:template="default" jcr:primaryType="jnt:wrapper" j:key="wikiPageWrapper"
                             j:applyOn="jnt:wikiPage">
                        <wrappercontent jcr:primaryType="jnt:contentList">
                            <row column="2col124" jcr:primaryType="jnt:row">
                                <col1 jcr:primaryType="jnt:contentList">
                                    <tabularList jcr:primaryType="jnt:tabularList">
                                        <content jcr:primaryType="jnt:contentList">
                                            <j:translation_en jcr:language="en" jcr:title_en="Content"
                                                              jcr:primaryType="jnt:translation"/>
                                            <mainResourceDisplay jcr:primaryType="jnt:mainResourceDisplay"
                                                                 j:referenceTemplate="default"/>
                                        </content>
                                        <contribute jcr:primaryType="jnt:contentList">
                                            <j:translation_en jcr:title_en="Contribute" jcr:language="en"
                                                              jcr:primaryType="jnt:translation"/>
                                            <mainResourceDisplay jcr:primaryType="jnt:mainResourceDisplay"
                                                                 j:referenceTemplate="contribute"/>
                                        </contribute>
                                        <history jcr:primaryType="jnt:contentList">
                                            <j:translation_en jcr:title_en="History" jcr:language="en"
                                                              jcr:primaryType="jnt:translation"/>
                                            <mainResourceDisplay jcr:primaryType="jnt:mainResourceDisplay"
                                                                 j:referenceTemplate="history"/>
                                        </history>
                                    </tabularList>
                                </col1>
                                <col2 jcr:primaryType="jnt:contentList"/>
                            </row>
                        </wrappercontent>
                    </wrapper>
                </wiki>
                <forum jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:template" changefreq="monthly" priority="0.5">
                    <j:translation_en jcr:title_en="Forum" jcr:language="en" jcr:primaryType="jnt:translation"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <row column="2col124" jcr:primaryType="jnt:row">
                            <col1 jcr:primaryType="jnt:contentList">
                                <boardIndex boardDescription="Board Description" jcr:primaryType="jnt:boardIndex"
                                            boardSubject="Board Subject"/>
                            </col1>
                            <col2 jcr:primaryType="jnt:contentList"/>
                        </row>
                    </pagecontent>
                    <wrapper jcr:mixinTypes="jmix:templateInformation" j:template="default"
                             jcr:primaryType="jnt:wrapper" j:key="forumWrapper" j:applyOn="jnt:topic jnt:thread">
                        <wrappercontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                            <row column="2col124" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row">
                                <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <mainResourceDisplay jcr:mixinTypes="jmix:templateInformation"
                                                         jcr:primaryType="jnt:mainResourceDisplay"
                                                         j:referenceTemplate="default"/>
                                </col1>
                                <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation"/>
                                </col2>
                            </row>
                        </wrappercontent>
                    </wrapper>
                </forum>
            </templates>
            <sitewrapper j:template="intranet.bodywrapper" jcr:primaryType="jnt:wrapper" j:key="bodywrapper">
                <logo jcr:primaryType="jnt:contentList"/>
                <simpleSearch jcr:primaryType="jnt:contentList">
                    <simpleSearchForm result="/templatesSet/templates-intranet/search-results"
                                      jcr:mixinTypes="jmix:templateInformation jmix:shareable"
                                      jcr:primaryType="jnt:simpleSearchForm" j:templateLocked="true"/>
                </simpleSearch>
                <topMenu jcr:primaryType="jnt:contentList">
                    <top-menu jcr:mixinTypes="jmix:templateInformation jmix:shareable" j:maxDepth="2"
                              jcr:primaryType="jnt:navMenu" j:baselineNode="/templatesSet/templates-intranet/home"
                              j:styleName="navigation"/>
                    <shortcuts jcr:mixinTypes="jmix:templateInformation jmix:shareable" jcr:primaryType="jnt:shortcuts"
                               j:templateLocked="true"/>
                </topMenu>
                <footer jcr:primaryType="jnt:contentList"/>
                <wrappercontent jcr:primaryType="jnt:contentList">
                    <mainResourceDisplay jcr:primaryType="jnt:mainResourceDisplay"/>
                </wrappercontent>
                <pagetitle jcr:primaryType="jnt:contentList"/>
            </sitewrapper>
            <userwrapper j:template="default" jcr:primaryType="jnt:wrapper" j:key="userwrapper" j:applyOn="jnt:user">
                <wrappercontent jcr:primaryType="jnt:contentList">
                    <tabularList jcr:primaryType="jnt:tabularList">
                        <my-profile jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:primaryType="jnt:translation" jcr:title_en="My Profile"
                                              jcr:language="en"/>
                            <user-profile jcr:primaryType="jnt:currentUserDisplay" j:referenceTemplate="myprofile"/>
                        </my-profile>
                        <my-content jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:primaryType="jnt:translation" jcr:language="en"
                                              jcr:title_en="My Content"/>
                            <currentUserDisplay jcr:primaryType="jnt:currentUserDisplay"
                                                j:referenceTemplate="mycontent"/>
                        </my-content>
                        <my-docspaces jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:primaryType="jnt:translation" jcr:title_en="My Docspaces"
                                              jcr:language="en"/>
                            <currentUserDisplay jcr:primaryType="jnt:currentUserDisplay"
                                                j:referenceTemplate="mydocspaces"/>
                        </my-docspaces>
                        <my-tasks jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:primaryType="jnt:translation" jcr:language="en"
                                              jcr:title_en="My Tasks"/>
                            <currentUserDisplay jcr:primaryType="jnt:currentUserDisplay"
                                                j:referenceTemplate="tasklist"/>
                        </my-tasks>
                        <my-bookmarks jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:primaryType="jnt:translation" jcr:title_en="My Bookmarks"
                                              jcr:language="en"/>
                            <currentUserDisplay jcr:primaryType="jnt:currentUserDisplay"
                                                j:referenceTemplate="bookmarks"/>
                        </my-bookmarks>
                    </tabularList>
                </wrappercontent>
            </userwrapper>
            <search-results jcr:mixinTypes="jmix:templateInformation jmix:sitemap" j:template="default"
                            jcr:primaryType="jnt:page" changefreq="monthly" priority="0.5">
                <j:translation_en j:template="default" jcr:primaryType="jnt:translation" changefreq="monthly"
                                  jcr:title_en="Search results" jcr:language="en" priority="0.5"/>
                <pagecontent jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList"
                             j:templateLocked="true">
                    <row1 column="1col16" jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:row"
                          j:templateLocked="true">
                        <col1 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList">
                            <results autoSuggest="true" pageSize="10" jcr:mixinTypes="jmix:templateInformation"
                                     jcr:primaryType="jnt:searchResults" autoSuggestHitCount="2"
                                     autoSuggestMinimumHitCount="2">
                                <j:translation_en autoSuggest="true" pageSize="10" jcr:primaryType="jnt:translation"
                                                  autoSuggestHitCount="2" jcr:language="en" jcr:title_en="Results"
                                                  autoSuggestMinimumHitCount="2"/>
                            </results>
                        </col1>
                        <col2 jcr:mixinTypes="jmix:templateInformation" jcr:primaryType="jnt:contentList"/>
                    </row1>
                </pagecontent>
            </search-results>
        </templates-intranet>
    </templatesSet>
</content>
