<jnt = 'http://www.jahia.org/jahia/nt/1.0'>

[jnt:paragraph] > jnt:mainContentContainer, jmix:structuredContent
bigText         insertText
sharedSmallText insertType (choicelist[resourceBundle]) = quote indexed=no < quote, exergue
sharedSmallText insertPosition (choicelist[resourceBundle]) = left indexed=no < left, right
integer         insertWidth = "150" indexed=no

[jnt:article] > jnt:container, jmix:structuredContent
smallText       title
bigText         intro
+ * (jnt:paragraph)