<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:test="http://www.apache.org/jackrabbit/test"
         xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <templates-system jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite" changefreq="monthly"
                          j:installedModules="templates-system" priority="0.5" j:siteType="templatesSet">
            <templates jcr:primaryType="jnt:templatesFolder">
                <base jcr:mixinTypes="jmix:renderable jmix:theme" jcr:primaryType="jnt:template" j:view="system">
                    <home jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:mixinTypes="jmix:style" jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:title="Links" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"/>
                            <Introduction jcr:primaryType="jnt:bigText">
                                <j:translation_en
                                        text="&lt;h1&gt;  &lt;em&gt;What do you want to do with Jahia today ?&lt;/em&gt;&lt;/h1&gt; "
                                        jcr:primaryType="jnt:translation" jcr:language="en"/>
                            </Introduction>
                            <login-row column="1col16" jcr:primaryType="jnt:row"/>
                            <row column="2col412" j:requiredPermissions="" jcr:mixinTypes="jmix:requiredPermissions"
                                 jcr:primaryType="jnt:row" j:requireLoggedUser="true"/>
                        </pagecontent>
                        <row-col1 jcr:primaryType="jnt:contentList">
                            <my-task j:skin="skins.box6" jcr:mixinTypes="jmix:skinnable"
                                     jcr:primaryType="jnt:contentList">
                                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="My Tasks"
                                                  jcr:primaryType="jnt:translation"/>
                                <currentUserTasks jcr:primaryType="jnt:currentUserTasks"/>
                            </my-task>
                            <application-shortcuts jcr:mixinTypes="jmix:renderable jmix:columns"
                                                   jcr:primaryType="jnt:contentList" j:view="columns" j:columns="2">
                                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                  jcr:title="Application shortcuts" jcr:primaryType="jnt:translation"/>
                                <contentList j:skin="skins.box6"
                                             jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                             jcr:primaryType="jnt:contentList" j:view="unordered" j:className="simple">
                                    <jahia-studio-200.png
                                            j:node="/templateSets/templates-system/templates/files/themes/default/jahia-studio-200.png"
                                            jcr:primaryType="jnt:imageReference"/>
                                    <gotoStudio jcr:primaryType="jnt:gotoStudio">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"/>
                                    </gotoStudio>
                                </contentList>
                                <go-to-the-server-administration j:skin="skins.box6"
                                                                 jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                                                 jcr:primaryType="jnt:contentList" j:view="unordered"
                                                                 j:className="simple">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:primaryType="jnt:translation"/>
                                    <jahia-ccp-200.png
                                            j:node="/templateSets/templates-system/templates/files/themes/default/jahia-ccp-200.png"
                                            jcr:primaryType="jnt:imageReference"/>
                                    <gotoAdmin jcr:primaryType="jnt:gotoAdmin"/>
                                </go-to-the-server-administration>
                                <go-the-global-united-content-man-1 j:skin="skins.box6"
                                                                    jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                                                    jcr:primaryType="jnt:contentList"
                                                                    j:className="simple" j:view="unordered">
                                    <j:translation_en jcr:language="en" jcr:title="United Content Manager"
                                                      jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"/>
                                    <gotoManager type="united content" jcr:primaryType="jnt:gotoManager"/>
                                </go-the-global-united-content-man-1>
                            </application-shortcuts>
                        </row-col1>
                        <row-col2 jcr:primaryType="jnt:contentList">
                            <listbutton jcr:mixinTypes="jmix:renderable jmix:columns" jcr:primaryType="jnt:contentList"
                                        j:view="columns" j:columns="1">
                                <j:translation_en jcr:language="en" jcr:title="listbutton" jcr:mixinTypes="mix:title"
                                                  jcr:primaryType="jnt:translation"/>
                                <my-websites-access-in-edit-mode j:skin="skins.box6"
                                                                 jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                                                 jcr:primaryType="jnt:contentList" j:view="unordered"
                                                                 j:className="simple">
                                    <j:translation_en jcr:mixinTypes="mix:title"
                                                      jcr:title="My websites (access in edit mode)"
                                                      jcr:primaryType="jnt:translation" jcr:language="en"/>
                                    <websites-list-in-edit-mode type="edit" jcr:primaryType="jnt:listSites">
                                        <j:translation_en jcr:language="en" jcr:title="websites list in edit mode"
                                                          jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"/>
                                    </websites-list-in-edit-mode>
                                </my-websites-access-in-edit-mode>
                                <my-websites-in-contribute-mode j:skin="skins.box6"
                                                                jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                                                jcr:primaryType="jnt:contentList" j:className="simple"
                                                                j:view="unordered">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:title="My websites (access in contribute mode)"
                                                      jcr:primaryType="jnt:translation"/>
                                    <websites-list-in-contribute-mode type="contribute" jcr:primaryType="jnt:listSites">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="websites list in contribute mode"
                                                          jcr:primaryType="jnt:translation"/>
                                    </websites-list-in-contribute-mode>
                                </my-websites-in-contribute-mode>
                                <go-the-global-united-content-man j:skin="skins.box6"
                                                                  jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                                                  jcr:primaryType="jnt:contentList" j:view="unordered"
                                                                  j:className="simple">
                                    <j:translation_en jcr:language="en" jcr:title="Manage Files (Document Manager)"
                                                      jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"/>
                                    <gotoManager type="document" jcr:primaryType="jnt:gotoManager"/>
                                </go-the-global-united-content-man>
                                <go-the-global-united-content-man-1 j:skin="skins.box6"
                                                                    jcr:mixinTypes="jmix:renderable jmix:listOrdered jmix:skinnable"
                                                                    jcr:primaryType="jnt:contentList"
                                                                    j:className="simple" j:view="unordered">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:title="Manage Content (Content Manager)"
                                                      jcr:primaryType="jnt:translation"/>
                                    <gotoManager type="content" jcr:primaryType="jnt:gotoManager"/>
                                </go-the-global-united-content-man-1>
                            </listbutton>
                        </row-col2>
                        <login-row-col1 j:skin="skins.box6" jcr:mixinTypes="jmix:style jmix:skinnable"
                                        jcr:primaryType="jnt:contentList">
                            <login jcr:primaryType="jnt:loginForm" j:displayRememberMeButton="true"
                                   j:displayLoginButton="true">
                                <j:translation_en jcr:language="en" jcr:title="Login"
                                                  jcr:primaryType="jnt:translation"/>
                            </login>
                        </login-row-col1>
                    </home>
                    <simple jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList"/>
                    </simple>
                    <header jcr:primaryType="jnt:contentList">
                        <header column="2col412" jcr:primaryType="jnt:row"/>
                    </header>
                    <footer jcr:primaryType="jnt:contentList">
                        <footer column="1col16" jcr:primaryType="jnt:row"/>
                    </footer>
                    <header-col1 jcr:primaryType="jnt:contentList">
                        <currentuser j:userView="welcome" jcr:primaryType="jnt:currentUserDisplay"/>
                    </header-col1>
                    <footer-col1 jcr:primaryType="jnt:contentList">
                        <bigText jcr:primaryType="jnt:bigText">
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation"
                                              text="&lt;p&gt;  &amp;copy; 2002-2011 Jahia Solutions Group - All rights reserved.&lt;/p&gt; "/>
                        </bigText>
                    </footer-col1>
                    <header-col2 jcr:primaryType="jnt:contentList">
                        <logo-jahia-xcm.png
                                j:node="/templateSets/templates-system/templates/files/themes/default/logo-jahia-xcm.png"
                                jcr:primaryType="jnt:imageReference">
                            <j:translation_fr jcr:language="fr" j:alternateText="logo-jahia-xcm"
                                              jcr:primaryType="jnt:translation"/>
                        </logo-jahia-xcm.png>
                    </header-col2>
                </base>
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder">
                            <photo_portrait.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="48"
                                                j:width="48">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </photo_portrait.png>
                            <my-portal-logo.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="50"
                                                j:width="129">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </my-portal-logo.png>
                            <jahia-studio-200.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="56"
                                                  j:width="200">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </jahia-studio-200.png>
                            <jahia-ccp-200.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="49"
                                               j:width="200">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </jahia-ccp-200.png>
                            <logo-jahia-xcm.png jcr:mixinTypes="jmix:image" j:height="60" jcr:primaryType="jnt:file"
                                                j:width="115">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </logo-jahia-xcm.png>
                        </default>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
            </templates>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <home jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" changefreq="monthly"
                  j:templateNode="/templateSets/templates-system/templates/base/home" priority="0.5">
                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Home"
                                  jcr:primaryType="jnt:translation"/>
            </home>
            <portlets jcr:primaryType="jnt:portletFolder"/>
        </templates-system>
    </templateSets>
</content>

