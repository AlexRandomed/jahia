<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>

    <!-- GWT resources-->
    <rule>
        <note>GWT CKEditor resources</note>
        <from>^/gwt/(.*)/ckeditor(.*?)/(.*)$</from>
        <to last="true">/modules/assets/javascript/ckeditor$2/$3</to>
    </rule>
    <rule>
        <note>GWT CSS</note>
        <from>^/gwt/org\.(.*)/(.*)\.css$</from>
        <to last="true">/gwt/resources/$2.css</to>
    </rule>
    <rule>
        <note>GWT images</note>
        <from>^/gwt/org\.(.*)/images/(.*)$</from>
        <to last="true">/gwt/resources/images/$2</to>
    </rule>
    <rule>
        <note>GWT CSS images</note>
        <from>^/gwt/resources/css/images/(.*)$</from>
        <to last="true">/gwt/resources/images/$1</to>
    </rule>
    <rule>
        <note>GWT clear.gif image</note>
        <from>^/gwt/org\.(.*)/clear.gif(.*)?$</from>
        <to last="true">/gwt/resources/clear.gif$2</to>
    </rule>
    <!-- end of GWT resources-->

</urlrewrite>