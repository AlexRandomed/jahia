
<user jcr:primaryType="jnt:user"
           xmlns:jcr="http://www.jcp.org/jcr/1.0"
           xmlns:j="http://www.jahia.org/jahia/1.0"
           xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" >

    <files jcr:primaryType="jnt:folder">
        <private jcr:primaryType="jnt:folder" >
            <j:acl jcr:primaryType="jnt:acl">
              <DENY_u_guest j:protected="false" j:principal="u:guest" j:roles="visitor" j:aceType="DENY" jcr:primaryType="jnt:ace" />
              <DENY_g_users j:protected="false" j:principal="g:users" j:roles="visitor" j:aceType="DENY" jcr:primaryType="jnt:ace" />
            </j:acl>

            <imports jcr:primaryType="jnt:importDropBox" />
        </private>
    </files>

    <portlets jcr:primaryType="jnt:portletFolder"/>

    <contents jcr:primaryType="jnt:contentFolder"/>

    <preferences jcr:primaryType="jnt:preferences"  >
    </preferences>

</user>