<user jcr:primaryType="jnt:userFolder"
           xmlns:jcr="http://www.jcp.org/jcr/1.0"
           xmlns:j="http://www.jahia.org/jahia/1.0"
           xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" >

    <files jcr:primaryType="jnt:folder">
        <private jcr:primaryType="jnt:folder" >
            <j:acl jcr:primaryType="jnt:acl">
                <!--<GRANT_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="" j:aceType="GRANT"/>-->
                <DENY_jcr_read_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="jcr:read" j:aceType="DENY"/>
                <DENY_jcr_write_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="jcr:write" j:aceType="DENY"/>
            </j:acl>

            <imports jcr:primaryType="jnt:importDropBox" />
        </private>
    </files>

    <mashups jcr:primaryType="jnt:folder"/>


    <preferences jcr:primaryType="jnt:preferences" j:hidden="true" >
        <j:acl jcr:primaryType="jnt:acl">
            <!--<GRANT_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="" j:aceType="GRANT"/>-->
            <!--<DENY_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="jcr:read jcr:write" j:aceType="DENY"/>-->
            <DENY_jcr_read_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="jcr:read" j:aceType="DENY"/>
            <DENY_jcr_write_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest" j:privileges="jcr:write" j:aceType="DENY"/>
        </j:acl>

        <simple jcr:primaryType="jnt:preferenceProvider" j:hidden="true" />
        <page jcr:primaryType="jnt:preferenceProvider" j:hidden="true" />
        <toolbar jcr:primaryType="jnt:preferenceProvider" j:hidden="true" />
        <portlet jcr:primaryType="jnt:preferenceProvider" j:hidden="true" />
        <layoutmanager jcr:primaryType="jnt:preferenceProvider" j:hidden="true" />
        <bookmarks jcr:primaryType="jnt:preferenceProvider" j:hidden="true" />
    </preferences>

</user>