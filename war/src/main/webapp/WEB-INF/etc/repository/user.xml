<user jcr:primaryType="jnt:userFolder"
           xmlns:jcr="http://www.jcp.org/jcr/1.0"
           xmlns:j="http://www.jahia.org/jahia/1.0"
           xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" j:applyAcl="self:RW">

    <files jcr:primaryType="jnt:folder">
        <private jcr:primaryType="jnt:folder" j:applyAcl="g:guest:--" >
            <imports jcr:primaryType="jnt:importDropBox" />
        </private>
    </files>

    <mashups jcr:primaryType="jnt:folder"/>


    <preferences jcr:primaryType="jnt:preferences" j:hidden="true" j:applyAcl="g:guest:--" >
        <simple jcr:primaryType="jnt:preferenceProvider" j:hidden="true" j:applyAcl="g:guest:--"/>
        <page jcr:primaryType="jnt:preferenceProvider" j:hidden="true" j:applyAcl="g:guest:--"/>
        <toolbar jcr:primaryType="jnt:preferenceProvider" j:hidden="true" j:applyAcl="g:guest:--"/>
        <portlet jcr:primaryType="jnt:preferenceProvider" j:hidden="true" j:applyAcl="g:guest:--"/>
        <layoutmanager jcr:primaryType="jnt:preferenceProvider" j:hidden="true" j:applyAcl="g:guest:--"/>
        <bookmarks jcr:primaryType="jnt:preferenceProvider" j:hidden="true" j:applyAcl="g:guest:--"/>
    </preferences>

</user>