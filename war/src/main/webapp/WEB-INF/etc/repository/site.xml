
<site jcr:primaryType="jnt:virtualsite"
           xmlns:jcr="http://www.jcp.org/jcr/1.0"
           xmlns:j="http://www.jahia.org/jahia/1.0"
           xmlns:jnt="http://www.jahia.org/jahia/nt/1.0">

    <files jcr:primaryType="jnt:folder" />

    <portlets jcr:primaryType="jnt:portletFolder" />

    <groups jcr:primaryType="jnt:groupsFolder">
        <site-administrators jcr:primaryType="jnt:group" j:hidden="false">
            <j:members jcr:primaryType="jnt:members"/>
        </site-administrators>
        <site-privileged jcr:primaryType="jnt:group" j:hidden="false">
            <j:members jcr:primaryType="jnt:members"/>
        </site-privileged>
    </groups>

    <contents jcr:primaryType="jnt:contentFolder" />

    <templates jcr:primaryType="jnt:templatesFolder"/>

    <tags jcr:primaryType="jnt:tagList" >
    </tags>

    <j:acl jcr:primaryType="jnt:acl">
      <GRANT_g_site-privileged j:protected="false" j:principal="g:site-privileged" j:roles="staging-viewer" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
      <GRANT_g_privileged j:protected="false" j:principal="g:privileged" j:roles="staging-viewer" j:aceType="DENY" jcr:primaryType="jnt:ace" />
      <GRANT_g_site-administrators j:principal="g:site-administrators" j:protected="false" j:roles="site-administrator" j:aceType="GRANT" jcr:primaryType="jnt:ace" />
    </j:acl>

</site>