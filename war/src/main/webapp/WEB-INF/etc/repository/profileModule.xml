<?xml version="1.0" encoding="UTF-8"?>
<templatesSet xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
              xmlns:jcr="http://www.jcp.org/jcr/1.0"
              xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
              xmlns:mix="http://www.jcp.org/jcr/mix/1.0" jcr:mixinTypes="jmix:sitemap"
              jcr:primaryType="jnt:virtualsite" changefreq="monthly" j:installedModules="templates-web" priority="0.5"
              j:siteType="templatesSet">
    <portlets jcr:primaryType="jnt:portletFolder"/>
    <files jcr:primaryType="jnt:folder"/>
    <contents jcr:primaryType="jnt:contentFolder"/>
    <templates jcr:primaryType="jnt:templatesFolder">
        <files jcr:primaryType="jnt:folder">
            <themes jcr:primaryType="jnt:folder">
                <default jcr:primaryType="jnt:folder">

                </default>
            </themes>
        </files>
        <contents jcr:primaryType="jnt:contentFolder"/>

        <base jcr:mixinTypes="jmix:renderable" j:view="apps" jcr:primaryType="jnt:template"
              j:theme="/templatesSet/files/themes/default">
            <user-base j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                <pagecontent jcr:primaryType="jnt:contentList">
                    <user-base-layout customColumn="3, 10, 3" divID="content" jcr:primaryType="jnt:customRow"
                                      principalColumn="2"/>
                </pagecontent>

                <user-base-layout-colContent jcr:primaryType="jnt:contentList">
                    <pagecontent j:allowedTypes="" jcr:primaryType="jnt:area"/>
                </user-base-layout-colContent>

                <user-base-layout-col1 jcr:primaryType="jnt:contentList">
                    <user-profile-aside-left j:allowedTypes="" jcr:primaryType="jnt:area"/>
                </user-base-layout-col1>

                <user-base-layout-col3 jcr:primaryType="jnt:contentList">
                    <user-profile-aside-right j:allowedTypes="" jcr:primaryType="jnt:area"/>
                </user-base-layout-col3>
            </user-base>
        </base>

    </templates>

</templatesSet>
