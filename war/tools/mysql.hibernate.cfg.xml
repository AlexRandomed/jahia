<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="dialect">org.hibernate.dialect.MySQL5InnoDBDialect</property>
		<mapping class="org.jahia.hibernate.model.JahiaDbTest" />
		<mapping class="org.jahia.hibernate.model.JahiaInstalledPatch" />
		<mapping class="org.jahia.hibernate.model.JahiaPwdPolicy" />
		<mapping class="org.jahia.hibernate.model.JahiaPwdPolicyRule" />
		<mapping class="org.jahia.hibernate.model.JahiaPwdPolicyRuleParam" />
		<mapping class="org.jahia.hibernate.model.JahiaVersion" />
		<mapping class="org.jahia.services.history.HistoryEntry" />
	</session-factory>
</hibernate-configuration>

