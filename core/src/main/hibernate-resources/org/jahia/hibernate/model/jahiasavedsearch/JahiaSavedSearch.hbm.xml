<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.jahia.hibernate.model.jahiasavedsearch.JahiaSavedSearch"
        table="jahia_savedsearch"
        lazy="false"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
        optimistic-lock="version"
    >
        <cache usage="nonstrict-read-write" />

        <id
            name="id"
            column="id_jahia_savedsearch"
            type="java.lang.Integer"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-JahiaSavedSearch.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="title"
            type="string"
            update="true"
            insert="true"
            access="property"
            column="title_jahia_savedsearch"
        />

        <property
            name="descr"
            type="string"
            update="true"
            insert="true"
            access="property"
            column="descr_jahia_savedsearch"
            length="250"
        />

        <property
            name="search"
            type="text"
            update="true"
            insert="true"
            access="property"
            column="search_jahia_savedsearch"
            length="16777216"
            not-null="true"
        />

        <property
            name="creationDate"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="creationdate_jahia_savedsearch"
        />

        <property
            name="ownerKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="owner_jahia_savedsearch"
        />

        <property
            name="searchViewHandlerClass"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="class_jahia_savedsearch"
        />

        <many-to-one
            name="jahiaSite"
            class="org.jahia.hibernate.model.JahiaSite"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
        >
            <column
                name="jahiaid_jahia_savedsearch" 
            />
        </many-to-one>

        <many-to-one
            name="jahiaAcl"
            class="org.jahia.hibernate.model.JahiaAcl"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
        >
            <column
                name="rights_jahia_search" 
            />
        </many-to-one>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-JahiaSavedSearch.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
