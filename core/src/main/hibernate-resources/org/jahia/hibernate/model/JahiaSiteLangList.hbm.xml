<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.jahia.hibernate.model.JahiaSiteLangList"
        table="jahia_site_lang_list"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
        optimistic-lock="version"
    >
        <cache usage="nonstrict-read-write" />

        <id
            name="id"
            column="id"
            type="java.lang.Integer"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-JahiaSiteLangList.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="site"
            class="org.jahia.hibernate.model.JahiaSite"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="site_id"
        />

        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="code"
            length="255"
        />

        <property
            name="rank"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="rank"
            length="11"
        />

        <property
            name="activated"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="activated"
        />

        <property
            name="mandatory"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            access="property"
            column="mandatory"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-JahiaSiteLangList.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
