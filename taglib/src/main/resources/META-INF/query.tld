    <tag>
        <description>Used to set, append or remove Query property value. Look at QUERY CORE PROPERTIES chapter in Container Query tags Document.</description>
        <name>setProperty</name>
        <tag-class>org.jahia.taglibs.query.SetPropertyTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>name of the property you want to append.</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>The parameter value, if null the parameter won't be set</description>
            <name>value</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                <p>how to set the parameter.</p>
                <p>values are :
                <ul>
                <li>set : Set and override any previous values</li>
                <li>append : Append the value to the current value separated by a comma, or set the value if it doesn't exist.</li>
                <li>remove : Remove the parameter</li>
                 </ul></p>]]>
            </description>
            <name>operation</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>Used inside a tag, which implements the ParamParent interface (e.g. SetPropertyTag, CreateFacetFilterTag) to add parameter values or do whatever the operation of the parent tag is.</description>
        <name>propertyValue</name>
        <tag-class>org.jahia.taglibs.query.PropertyValueTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>The parameter value, if null the parameter won't be set</description>
            <name>value</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>

    <tag>
        <description>Used create a logs query</description>
        <name>logsQuery</name>
        <tag-class>org.jahia.taglibs.query.logs.LogsQueryTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description><![CDATA[
                    <p>This is the name of the query template to use.</p>
                    <p>Required</p>
                    ]]>
            </description>
            <name>logsQueryName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>A max number of returned rows can be defined. Unlimited by default.</p>
                    ]]>
            </description>
            <name>dbMaxLimit</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>Defines the max size of the result object <code>LogsResultList</code>. This max size is usually
                    small (5,10,50) and is used to only keep the first 5, 10 or 50 log entries after
                    applying ACL check and/or time based publishing check.</p>
                    ]]>
            </description>
            <name>maxSize</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>To ensure that you want the first and only one log entry by ContentObject, this attribute must be
                    to true.</p>
                    <p>Default is true.</p>
                    ]]>
            </description>
            <name>uniqueContentObject</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>This flag is used to filter content based on their Time based publishing status.</p>
                    <p>values can be :<ul>
                    <li>TimeBasedPublishingState.IS_STATE_LOAD_FLAG</li>
                    <li>TimeBasedPublishingState.NOT_VALID_STATE_LOAD_FLAG</li>
                    <li>TimeBasedPublishingState.EXPIRED_STATE_LOAD_FLAG</li>
                    <li>TimeBasedPublishingState.EXPIRED_OR_NOT_VALID_STATE_LOAD_FLAG</li>
                    <li>TimeBasedPublishingState.ALL_STATES_LOAD_FLAG</li>
                    </ul></p>
                    ]]>
            </description>
            <name>timeBasedPublishingLoadFlag</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>"true" if ACL check must be performed.</p>
                    <p>Default is true</p>
                    ]]>
            </description>
            <name>checkACL</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>Unique id used to identify the LogsQuery object.</p>
                    <p>The object representing the current LogQuery is added to the pageContext under this name
                    in the page scope</p>
                    ]]>
            </description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>Used to expose a single LogEntryBean to the page context</description>
        <name>logEntryBean</name>
        <tag-class>org.jahia.taglibs.query.logs.LogEntryBeanTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description><![CDATA[
                    <p>Name of the LogEntryItem object you generaly iterate on.</p>
                    ]]>
            </description>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    <p>Unique id used to identify the AbstractJahiaObjectBean object.</p>
                    <p>The object representing the current AbstractJahiaObjectBean is added to the pageContext under this name
                    in the page scope</p>
                    ]]>
            </description>
            <name>beanId</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <example>
            <![CDATA[
                <query:executeLogsQuery id="queryResult">
                    <c:forEach var="logEntry" items="${queryResult}">
                        <query:logEntryBean id="logEntry" beanId="containerBean">
                            <content:container id="new" srcBeanId="containerBean"/>
                        </query:logEntryBean>
                    </c:forEach>
                </query:executeLogsQuery>
            ]]>
        </example>
    </tag>
    <tag>
        <description>Used effectively execute a logs query</description>
        <name>executeLogsQuery</name>
        <tag-class>org.jahia.taglibs.query.logs.ExecuteLogsQueryTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description><![CDATA[
                     <p>Unique id used to identify the LogsResultList object.</p>
                     <p>The object representing the current LogsResultList is added to the pageContext</p>
                     ]]>
             </description>
             <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[
                    The pageContext scope under which to expose  the LogsResultList. The Default scope is
                    PageContext.PAGE_SCOPE if null. Values can be "page", "session", "application"
            ]]>
            </description>
            <name>scope</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>used to set the content definition names to the parent logsQuery tag</description>
        <name>contentDefinitionNamesConstraint</name>
        <tag-class>org.jahia.taglibs.query.logs.ContentDefinitionNamesConstraintTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>A comma separated list of Content Defintion Names. Required.</description>
            <name>definitionNames</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>used to set the usernames to the parent logsQuery tag</description>
        <name>usernamesConstraint</name>
        <tag-class>org.jahia.taglibs.query.logs.UsernamesConstraintTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>A comma separated list of usernames. Required.</description>
            <name>usernames</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>Create a ContainerSorter using a LogsQuery instance</description>
        <name>createContainerSorterFromLogsQuery</name>
        <tag-class>org.jahia.taglibs.query.CreateContainerSorterFromLogsQueryTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Name of the logQuery on which you want to create a ContainerSorter</description>
            <name>logsQueryId</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>Display a category selection drop-down, or other, in order to allow filtering by categories.
        </description>
        <name>categoryFilter</name>
        <tag-class>org.jahia.taglibs.template.category.GetCategoryChildrenTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>define the start of category tree</description>
            <name>startingCategoryKey</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>default is true. Display or not the selector</description>
            <name>display</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Maximum level</description>
            <name>level</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>name of the selector</description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>name of the selected category</description>
            <name>selected</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>Display ressource key message if selector contains no category</description>
            <name>messageKey</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>Css class name embed by the surrounding &lt;div&gt;</description>
            <name>cssClassName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>default true, display or not the root category in selector</description>
            <name>displayRootCategory</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
    </tag>
