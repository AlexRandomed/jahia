    <tag>
        <description>Used to define a full text search Constraint</description>
        <name>fullTextSearch</name>
        <tag-class>org.jahia.taglibs.query.FullTextSearchTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description><![CDATA[
                    <p>a <code>term</code> not preceded by "-" (minus) is satisfied only if the value contains that term.</p>
                    <p>a <code>term</code> preceded by "-" (minus) is satisfied only if the value does not contain that term.</p>
                    <p>Terms separated by withespace are implicity "ANDed".</p>
                    <p>"AND" has higher precedence that "OR"</p>
                    <p>Within a term, each double quote ("),  minus sign (-), and backslah (\) must be preceded by a backslash (\)</p>
                    <p>This parameter is required</p>
                    <p>look at <a href="http://lucene.apache.org/java/2_3_2/queryparsersyntax.html">http://lucene.apache.org/java/2_3_2/queryparsersyntax.html</a> for more information</p>
                    ]]>
            </description>
            <name>searchExpression</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[
                        <p>The search can be limited to a property.</p>
                        <p>If null or empty, the search is done everywhere</p>
                ]]>
            </description>
            <name>propertyName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                Name of the selector.
            </description>
            <name>selectorName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>        
    </tag>
    <tag>
        <description>Used to define an ordering. You can set multiple sortBy tag in the same query. </description>
        <name>sortBy</name>
        <tag-class>org.jahia.taglibs.query.SortByTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>
                <![CDATA[
                        <p>The name of the property on which to order.</p>
                        <p>Required.</p>
                ]]>
            </description>
            <name>propertyName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
             <description><![CDATA[
                     <p>Default is "asc" for ascending order, "desc" for descending order.</p>
                     ]]>
            </description>
            <name>order</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                Name of the selector.
            </description>
            <name>selectorName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <description>Used to define a Selector</description>
        <name>selector</name>
        <tag-class>org.jahia.taglibs.query.SelectorTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description><![CDATA[
                <p>The node type name.</p>
                <p>With Jahia Content, this is the Jahia Content Definition name (i.e. Jahia Container Definition name
                for Jahia Container)</p>
                <p>When you want to select all Jahia Containers no matter of their definition names, you can use the
                node type constant name <code>JahiaQueryObjectModelConstants.JAHIA_CONTAINER_NODE_TYPE</code>.</p> ]]>
            </description>
            <name>nodeTypeName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                Name of the selector. Required.
            </description>
            <name>selectorName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
        <example>
            &lt;query:selector selectorName="containers" nodeTypeName="${queryConstants.JAHIA_CONTAINER_NODE_TYPE}"/&gt;
        </example>
    </tag>
